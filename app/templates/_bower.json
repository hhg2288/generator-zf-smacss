{
  "name": "<%= _.slugify(projName) %>",
  "version": "0.0.1",
  "authors": [
    "Horacio Herrera <me@hherrerag.com>"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "source/components"
  ],
  "devDependencies": {
    "jquery"        : "*"
    <% if (includeFoundation) { %>
      ,"foundation"    : "*"
    <% } %>
    <% if (includeRetina) { %>
      ,"retina.js"     : "*"
    <% }
    if (includeParallax) { %>
      ,"jquery.superscrollorama"     : "*"
    <% } %>
  }
}

